{
  "compilerOptions": {
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "pages/lessons/easyEnglish/dialogue.js",
    "modules/shared/ProtectPage/index.jsx",
    "modules/shared/Audioplayer/index.jsx",
    "modules/shared/api/getEverydayProgress.js",
    ".next/types/**/*.ts"
, "modules/study/withTeacher/teacher-side/TeacherCompensations"  ],
  "exclude": [
    "node_modules"
  ]
}
